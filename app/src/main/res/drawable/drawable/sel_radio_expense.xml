<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape><solid android:color="@color/accent_1"/>
            <corners android:radius="16dp"/><stroke android:width="2dp" android:color="@color/accent_1"/></shape>
    </item>
    <item>
        <shape><solid android:color="@color/white"/>
            <corners android:radius="16dp"/><stroke android:width="1dp" android:color="@color/neutral_200"/></shape>
    </item>
</selector>
